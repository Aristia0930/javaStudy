package com.market.bookitem;

public class Book extends Item{
	private String author;
	private String description;
	private String category;
	private String releaseDate;

	public Book(String booId, String name, int unitPrice) {
		super(booId, name, unitPrice);
		// TODO Auto-generated constructor stub
	}
	
	public Book(String booId, String name, int unitPrice , String author, String description, String category,String releaseDate) {
		super(booId, name, unitPrice);
		this.author=author;
		this.description=description;
		this.category=category;
		this.releaseDate=releaseDate;
		// TODO Auto-generated constructor stub
	}
	
	

	@Override
	String getBookId() {
		// TODO Auto-generated method stub
		return booId;
	}
	
	void setBookId(String booId) {
		this.booId=booId;
		
	}

	@Override
	String getName() {
		// TODO Auto-generated method stub
		return name;
	}
	
	public void setName(String booId) {
		this.name=booId;
		
	}
	
	


	@Override
	int getUnitPrice() {
		// TODO Auto-generated method stub
		return unitPrice;
	}
	
	public void setUnitPrice(int unitPrice) {
		this.unitPrice=unitPrice;
		
	}

	
	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getReleaseDate() {
		return releaseDate;
	}

	public void setReleaseDate(String releaseDate) {
		this.releaseDate = releaseDate;
	}
	
	








	

}
